package org.anveshgadde.multidimensionalarrays;

public class MultiDimeArr {

	public static void main(String[] args) {

		int[][] numArray = { { 1, 2, 3 }, { 4, 5, 6 } };

		int[][] secArray = { { 6, 5, 4 }, { 3, 2, 1 } };

		int[][] resArray = new int[2][3];

		for (int i = 0; i < numArray.length; i++) {
			for (int j = 0; j < secArray.length; j++) {
				resArray[i][j] = numArray[i][j] + secArray[i][j];
				
				System.out.println("Array element values are: " +resArray[i][j]);
			}
			System.out.println(System.getProperty("line.separator"));
		}

	}

}
