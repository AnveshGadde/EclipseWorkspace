/**
 * 
 */
package org.anveshgadde.wrapperimplementation;

/**
 * @author anveshg
 *
 */
public class WrapperImpl {

	public static void main(String[] args) {

		Integer inte = new Integer(33);

		int myInt = inte.intValue();
		
		char[] name = {'A', 'n', 'v', 'e', 's', 'h'};
		
		String userName = new String(name);
		
		System.out.println("Wrapping before: " + inte + " after wrapping: " + myInt+" with my name as "+userName);
	}

}
