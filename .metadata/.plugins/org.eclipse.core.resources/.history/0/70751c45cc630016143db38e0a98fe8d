/**
 * 
 */
package org.anveshgadde.interfaceimplementable;

/**
 * @author anveshg
 *
 */

interface Print {
	void printCricket();

	void printFootball();

	void printMovies();

	void printSongs();
}

abstract class applicable implements Print {
	public void printCricket() {
		System.out.println("Cricket is playing!");
	}
}
