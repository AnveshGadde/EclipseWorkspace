	public Object[] getHeaders() {
		return this.headers;
	}

	public Object[][] getData() {
		return this.data;
	}

	private void getQueryResults() {

		Statement stmt = null;
		ResultSet rs;

		ArrayList<Object> tempTable = new ArrayList<>();

		ArrayList<Object> tempRow;

	}
}
