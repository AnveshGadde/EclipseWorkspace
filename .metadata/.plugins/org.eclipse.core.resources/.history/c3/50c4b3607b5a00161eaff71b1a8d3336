package org.anveshgadde.datatypes;

public class Datatype {
	
	public static void main(String[] args){
		
		int myNumber = 5;
		float myFloat = 5.5f;
		double myDouble = 5.5;
		String myName = "anveshgadde";
		char myInitial = 'G';
		
		boolean myBoolean = true;
		
		byte myByte = 0xfd;
		
		
		
	}

}
